{
  "name": "flipboxfactory/craft-hubspot",
  "description": "HubSpot integration for Craft CMS",
  "version": "3.0.0",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "cms",
    "craftcms",
    "hubspot",
    "yii2"
  ],
  "require": {
    "php": ">=8.0",
    "craftcms/cms": "~4.0",
    "flipboxdigital/hubspot": "dev-feature/php8",
    "flipboxfactory/craft-ember": "^2.1",
    "flipboxfactory/craft-integration": "dev-feature/php8",
    "flipboxfactory/craft-psr3": "dev-feature/php8"
  },
  "require-dev": {
    "codeception/codeception": "^5.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\hubspot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\hubspot\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/craft-hubspot/issues?state=open",
    "source": "https://github.com/flipboxfactory/craft-hubspot",
    "docs": "https://github.com/flipboxfactory/craft-hubspot",
    "rss": "https://github.com/flipboxfactory/craft-hubspot/commits/master.atom"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "name": "HubSpot",
    "handle": "flipbox-hubspot",
    "class": "flipbox\\craft\\hubspot\\HubSpot",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/craft-hubspot/master/CHANGELOG.md",
    "documentationUrl": "https://hubspot.flipboxfactory.com",
    "hasCpSettings": true,
    "hasCpSection": true,
    "schemaVersion": "1.3.1",
    "branch-alias": {
      "dev-master": "1.2.x-dev",
      "dev-develop": "1.2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
